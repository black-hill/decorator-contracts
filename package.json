{
  "name": "ts-code-contracts",
  "version": "0.0.0",
  "description": "Design By Contract for TypeScript and ECMAScript classes",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run clean && npm run type-check && npm run build-types",
    "build": "parcel build src/index.ts",
    "build-types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist",
    "clean-full": "rimraf dist && rimraf .cache && rimraf node_modules",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://thenewobjective@dev.azure.com/thenewobjective/ts-code-contracts/_git/ts-code-contracts"
  },
  "keywords": [
    "contract",
    "contracts",
    "Dbc",
    "test",
    "assert",
    "require",
    "invariant"
  ],
  "author": "Michael L Haufe <tno@thenewobjective.com> (https://thenewobjective.com)",
  "license": "UNLICENSED",
  "jest": {
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {}
}
